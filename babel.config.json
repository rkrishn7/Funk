{
    "plugins": [
        "@babel/proposal-class-properties",
        ["@babel/plugin-proposal-decorators", {
            "decoratorsBeforeExport": true
        }],
        "@babel/plugin-proposal-export-default-from"
    ],
    "presets": [
        ["@babel/env", {
            "targets": {
                "browsers" : "last 2 versions"
            },
            "modules": false
        }],
        "@babel/react",
        "@babel/typescript"
    ]
}